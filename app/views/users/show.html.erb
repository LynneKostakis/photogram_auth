<div class="row">
  <div class="col-md-8 col-md-offset-2">
    <div class="jumbotron">
      <h3>
        <%= @user.username %>
      </h3>
    </div>
  </div>
</div>
  
  <ul>
  <% Photo.where(:user_id => @user.id).each do |photo| %>
        <li><%= photo.caption %></li>
        <li><%= photo.image %></li>
        <li><%= photo.user_id %></li>
  <% end %>

</ul>
  
